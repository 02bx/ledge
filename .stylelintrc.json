{
  "extends": "stylelint-config-standard",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "no-empty-source": null,
    "selector-type-no-unknown": null,
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true
  },
  "ignore": [
    "custom-elements",
    "default-namespace"
  ]
}